<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity OrderDiscount.
    -->
    <changeSet id="20251125112828-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="discount_id"
                                 baseTableName="order_discount"
                                 constraintName="fk_order_discount__discount_id"
                                 referencedColumnNames="id"
                                 referencedTableName="discount"/>

        <addForeignKeyConstraint baseColumnNames="order_id"
                                 baseTableName="order_discount"
                                 constraintName="fk_order_discount__order_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_order"/>
    </changeSet>
</databaseChangeLog>
